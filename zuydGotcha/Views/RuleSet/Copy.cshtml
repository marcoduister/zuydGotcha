@model Models.RuleSet

@{
    ViewBag.Title = "RuleSet Kopieren";
}

<h2>RegelLijst Kopiëren</h2>

<h3>Weet u zeker dat u deze regellijst wilt kopiëren?</h3>
<div>
    <hr/>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(model => model.RuleSet_Name)
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.RuleSet_Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.RuleSet_Name)
            </dd>
        </dl>

        <div class="form-actions no-color">
            <input type="submit" value="Kopieren" class="btn btn-default" /> |
            @Html.ActionLink("Terug", "Index")
        </div>
    }
</div>

